@charset "utf-8";
/* CSS Document */

/**************sign up page css start here***************/

#signup {
    margin: 5% 0 0 0;
    padding: 2% 2% 2% 0;
    background: #FFFFFF;
    box-shadow: 0 0 5px 0 #CCC;
    border: solid 1px #CCC;
    -moz-border-radius: 10px;
    -webkit-border-radius: 10px;
    border-radius: 10px;
}

.board {
    background: #FFF;
    border-right: solid 5px #0075ba;
    min-height: 625px;
}

    .board h1 {
        color: #0075ba;
        text-align: center;
    }

    .board h3 {
        text-align: center;
    }

.signup-form .form-group {
    margin: 0;
    padding: 20px 10px;
}
/*.signup-form .control-label{font-weight:600 !important;}*/
.signup-form .panel {
    border-left: none;
    border-right: none;
    border-radius: 0 !important;
    box-shadow: none;
}

.signup-form .panel-heading {
    padding: 0;
}

    .signup-form .panel-heading a.first-time {
        background: url(../images/plus-icon2.png) 96% center no-repeat;
    }

    .signup-form .panel-heading a {
        display: block;
        padding: 10px 15px;
        background: url(../images/minus-icon2.png) 96% center no-repeat;
        text-decoration: none;
    }

        .signup-form .panel-heading a::after {
            content: attr(data-mandatory) " ";
            font-size: 12px;
            color: #a94442;
            position: absolute;
            right: 10%;
        }
        /*.signup-form .panel-heading a::after{
	content:"{* field are mandatory}";
	font-size:12px;
	color:#a94442;
	position:absolute;
	right:10%;
	}*/
        .signup-form .panel-heading a.first {
            background: url(../images/minus-icon2.png) 96% center no-repeat;
        }

        .signup-form .panel-heading a.collapsed {
            background: url(../images/plus-icon2.png) 96% center no-repeat !important;
        }

        .signup-form .panel-heading a.showCollapsed {
            background: url("../images/minus-icon2.png") 96% center no-repeat !important;
        }

#payment-option {
    text-align: center;
}

    #payment-option img {
        width: 12%;
        height: auto;
    }

.sep-line {
    border-bottom: 1px dotted #a5defa;
}

.board .nav-tabs {
    position: relative;
    margin: 40px auto;
    margin-bottom: 0;
    border-bottom: 1px solid #a5defa;
    box-sizing: border-box;
}

.board i {
    padding-right: 0;
}

.board > div.board-inner {
    background: #fafafa url(http://subtlepatterns.com/patterns/geometry2.png);
    background-size: 30%;
}

p.narrow {
    width: 60%;
    margin: 10px auto;
}

.liner {
    height: 2px;
    background: #ddd;
    position: absolute;
    width: 80%;
    margin: 0 auto;
    left: 0;
    right: 0;
    top: 43%;
    z-index: 1;
}

.nav-tabs > li.active > a, .nav-tabs > li.active > a:hover, .nav-tabs > li.active > a:focus {
    background: #fec413;
    color: #FFF;
}

span.round-tabs {
    width: 70px;
    height: 70px;
    line-height: 70px;
    display: inline-block;
    border-radius: 100px;
    background: white;
    z-index: 2;
    position: absolute;
    left: 0;
    text-align: center;
    font-size: 25px;
    background: #fec413;
    color: #FFF;
}



    span.round-tabs.four {
        color: #f1685e;
        border: 2px solid #f1685e;
    }

li.active span.round-tabs.four {
    background: #fff !important;
    border: 2px solid #ddd;
    color: #f1685e;
}

span.round-tabs.five {
    color: #999;
    border: 2px solid #999;
}

li.active span.round-tabs.five {
    background: #fff !important;
    border: 2px solid #ddd;
    color: #999;
}

.nav-tabs > li.active > a span.round-tabs {
}

.nav-tabs > li {
    width: 33.3%;
    text-align: center;
}
/*li.active:before {
    content: " ";
    position: absolute;
    left: 45%;
    opacity:0;
    margin: 0 auto;
    bottom: -2px;
    border: 10px solid transparent;
    border-bottom-color: #fff;
    z-index: 1;
    transition:0.2s ease-in-out;
}*/
li:after {
    content: " ";
    position: absolute;
    left: 45%;
    opacity: 0;
    margin: 0 auto;
    bottom: 0px;
    border: 5px solid transparent;
    border-bottom-color: #a5defa;
    transition: 0.1s ease-in-out;
}

li.active:after {
    content: " ";
    position: absolute;
    left: 45%;
    opacity: 1;
    margin: 0 auto;
    bottom: 0px;
    border: 10px solid transparent;
    border-bottom-color: #a5defa;
}

.nav-tabs > li a {
    width: 70px;
    height: 70px;
    margin: 20px auto 5px auto;
    border-radius: 100%;
    padding: 0;
}

    .nav-tabs > li a:hover {
        background: transparent;
    }

.tab-content {
}

.tab-pane {
    position: relative;
    padding-top: 20px;
}

.tab-content .head {
    font-family: 'Roboto Condensed', sans-serif;
    font-size: 25px;
    text-transform: uppercase;
    padding-bottom: 10px;
}

.btn-outline-rounded {
    padding: 10px 40px;
    margin: 20px 0;
    border: 2px solid transparent;
    border-radius: 25px;
    font-weight: 600;
}

.btn.green {
    background-color: #5cb85c;
    /*border: 2px solid #5cb85c;*/
    color: #ffffff;
}

#billing-add {
    margin: 10px 20px;
    display: none;
}


@media( max-width : 585px ) {
    span .round-tabs;

{
    font-size: 16px;
    width: 50px;
    height: 50px;
    line-height: 50px;
}

.tab-content .head {
    font-size: 20px;
}

.nav-tabs > li a {
    width: 50px;
    height: 50px;
    line-height: 50px;
}

li.active:after {
    content: " ";
    position: absolute;
    left: 35%;
}

.btn-outline-rounded {
    padding: 12px 20px;
}

}

#errmsg {
    background: #edf8fd;
    display: none;
    color: #A94442;
    padding: 3px 0;
    position: absolute;
    width: 85%;
    text-align: left;
}

#basicplan {
    font-weight: 600;
    font-size: 15px;
    text-align: left;
    margin-bottom: 10px;
    color: #0075ba;
    /*padding:5px;
	background:#c5e9fa;
	margin:0;
	-moz-border-radius:3px;
	-webkit-border-radius:3px;
	border-radius:3px;*/
}

#final_plan {
    position: relative;
    padding: 2% 2% 2% 15%;
    background: #ffd36b;
    -moz-border-radius: 3px;
    -webkit-border-radius: 3px;
    border-radius: 3px;
    margin-top: 10px;
    color: #575555;
    font-size: 15px;
    font-weight: 600;
    text-align: left;
    display: block;
}

    #final_plan:before {
        content: attr(data-icon);
        font-family: FontAwesome;
        font-style: normal;
        font-weight: normal;
        font-size: 200%;
        position: absolute;
        left: 4%;
    }

@media (max-width:767px) {
    .liner {
        top: 50%;
    }

    .nav-tabs > li a {
        margin: 15px auto;
    }

    .board {
        border-right: none;
        min-height: 200px;
    }
}

/**************sign up page css end here***************/

/*Validation error -Nutan*/

.input-validation-error {
    margin-top: 8px;
}
