#mypetmenu {
	text-align: center;
	margin-bottom:10px;
}

#mypetmenu a.btn-nav {
    background-color: #fff;
    border: 1px solid #e0e1db;
    box-sizing: border-box;         /* Opera/IE 8+ */
	color:#0073b6;
	box-shadow: 20px 0 30px -8px #f2f2f2 inset, -20px 0 16px -8px #f2f2f2 inset;
	position:relative;
}
#mypetmenu a.btn-nav:hover {
    color: #fdc312;
    cursor: pointer;
    transition: color 1s;
}
#mypetmenu a.btn-nav.active {
    color: #fdc312;
    padding: 2px;
	border-top: 6px solid #fdc312;
	border-bottom: 6px solid #fdc312;
    border-left: 0;
    border-right: 0;
    box-sizing:border-box;
    transition: border 0.3s ease-out, color 0.3s ease 0.5s;
    animation: pulsate 1.2s linear infinite;
}
#mypetmenu a.btn-nav.active:before {
	content: '';
	position: absolute;
	border-style: solid;
	border-width: 6px 6px 0;
	border-color: #fdc312 transparent;
	display: block;
	width: 0;
	z-index: 1;
	margin-left: -6px;
	top: 0;
	left: 50%;
}
#mypetmenu a.btn-nav .fa {
    padding-top: 16px;
	font-size: 40px;
}
#mypetmenu a.btn-nav.active p {
    margin-bottom: 8px;
}
@-webkit-keyframes pulsate {
 50% { color: #585858; }
}
@keyframes pulsate {
 50% { color: #585858; }
}
#petnavthumb {
	border-radius:50%;
	width:45px;
	height:45px;
	position:relative;
	margin:11px auto 0 auto;
    transition: border 0.3s ease-out, color 0.3s ease 0.5s;
	
	}
#petnavthumb img{
	width:43px;
	height:43px;
	border-radius:50%;
	border: 2px solid #0073b6;
    transition: border 0.3s ease-out, color 0.3s ease 0.5s;
	box-shadow:0 0 50px 0 #F8181C inset;
	}
#petnavthumb:before{
	content: '';
	position: absolute;
	border-top: 6px solid transparent;
	border-bottom: 6px solid transparent;	
	border-left: 6px solid #0073b6;
	display: block;
	width: 0;
	z-index: 500;
	top: 15px;
	right: -4px;
	}
#mypetmenu a.btn-nav:hover #petnavthumb:before, #mypetmenu a.btn-nav.active #petnavthumb:before{
	border-left: 6px solid #fdc312;
	}
#mypetmenu a.btn-nav:hover #petnavthumb img, #mypetmenu a.btn-nav.active #petnavthumb img{
	border: 2px solid #fdc312;
	}
#mypetmenu a.btn-nav.active #petnavthumb img{
    animation: pulsateb 1.2s linear infinite;
	}
@-webkit-keyframes pulsateb {
 50% { border-color: #585858; }
}
@keyframes pulsateb {
 50% { border-color: #585858; }
}
#mypetmenu a.btn-nav.active #petnavthumb:before{
    animation: pulsatec 1.2s linear infinite;
	}
@-webkit-keyframes pulsatec {
 50% { border-left: 6px solid #585858; }
}
@keyframes pulsatec {
50% { border-left: 6px solid #585858; }
}	
	
