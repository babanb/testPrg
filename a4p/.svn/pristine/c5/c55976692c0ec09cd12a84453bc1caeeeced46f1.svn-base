@charset "utf-8";
/* CSS Document */
body {
	background: url(../images/bodybg.jpg) top left repeat;
	overflow-x: hidden;
}
#loader_container {
	background: rgba(253,253,253,1.00) url(../images/loader.gif) center center no-repeat;
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	z-index: 5000;
}
a {outline:none !important;}
#top-text {
	text-align: center;
	font-size: 12px;
	color: #FFF;
	padding: 7px 0;
}
.space1 {
	height: 15px;
}
.hidggden-ani {
	opacity: 0;
	
}
.visible-ani {
	opacity: 1;
}
i {
