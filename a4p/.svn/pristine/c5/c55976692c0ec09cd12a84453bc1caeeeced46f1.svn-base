}
#leftSidebar {
	padding: 31px 0;
	/*width:305px;*/	
	min-height: 400px;
	position: absolute;
	left: 0;
	top: 0;
	color: #eaf7ff;
}
ul.leftMenuicon {
	width: 50px;
	position: relative;
	text-align: center;
	margin: 0 15px 0 0;
	padding: 0;
	border-top: solid 1px #0664a4;
}
ul.leftMenuicon li {
	margin: 0;
	list-style: none;
	position: relative;
}
ul.leftMenuicon li span {
	width: 25px;
	height: 25px;
	line-height: 25px;
	text-align: center;
	vertical-align: middle;
	border-radius: 50%;
	position: absolute;
	top: -2px;
	right: 2px;
	background: #f36f05;
	color: #FFF;
	font-size: 12px;
}
ul.leftMenuicon a {
	display: block;
	line-height: 40px;
	text-align: center;
	height: 40px;
	font-size: 24px;
	color: #eaf7ff;
	border-bottom: solid 1px #0664a4;
	text-decoration:none;
}
ul.leftMenuicon a i {
	padding-right: 0;
}
#notification-container {
	width:100%;
	height:100%;
	background:rgba(0,0,0,0.30);
	position:fixed;
	top:0;
	left:0;
	display:none;
	z-index:500;
	}
#notification-container .list-group {
	box-shadow:0 0 10px 0 rgba(0,0,0,0.50);
	width:500px;
	margin:100px auto;
	position:relative;
	background:#d9edf7;
	}
#notification-container .list-group h4{text-align:center; padding-top:30px;}
#notificntClose {
	position:absolute;
	top:10px;
	right:10px;
	width:30px;
	height:30px;
	font-size:18px;
	color:#707070;
	cursor:pointer;
	}
#notification-container .list-group .list-group-item {border-color:#FFFFFF;}
#notification-container .list-group i{color:#5aa101; border-right:solid 1px #5aa101; width:30px; padding-right:0px; margin-right:10px;}
#notification-container .list-group a.seeAllnoti {
	text-align:center; 
	background:#b3e1f8;
	font-weight:600;
	}
.notiList {background:none !important; font-size:14px !important;}
.notiList i {padding-right:8px !important;}
.notiList tr td:last-child {font-size:12px !important;}
ul.leftMenu {
	margin: 1px 0 0 0;
	padding: 0;
	display: block;
	width: 220px
}
ul.leftMenu li {
	list-style: none;
}
ul.leftMenu li a {
	display: block;
	background: url(../images/dark-bg.png) top left repeat;
	font-size: 17px;
	color: #eaf7ff;
	height: 40px;
	border-bottom: solid 1px #0664a4;
	padding: 0 10px;
	line-height: 40px;
	font-weight: 300;
	box-shadow: -6px 0 5px -5px #014471 inset;
	text-decoration: none;
	position: relative;
	transition: padding 0.2s;	
}
ul.leftMenu li .arrow-left {
	width: 8px;
	height: 14px;
	background: url(../images/menu-rightArrow.png) top left no-repeat;
	margin: auto;
	position: absolute;
	top: 0;
	bottom: 0;
	left: -8px;
	transition: width 0.2s, left 0.2s, transform 0.2s;
}
ul.leftMenu li:hover .arrow-left {
	left: -5px;
	width: 5px;
}
ul.leftMenu li:hover a {
	padding-left: 15px;
}
ul.leftMenuicon a.active, ul.leftMenu a.active {
	color: #fec412;
	text-decoration: none;
}
ul.leftMenu ul.submenu {
	margin:0;
	padding:0;
	border-bottom: solid 1px #0664a4;
	display:none;
	}
ul.leftMenu ul.submenu a{
	border-bottom:none;
	font-size:14px;
	height: 30px;
	line-height:30px;
	color:#aadcfe;
	border-top: solid 1px #0664a4;
	}
ul.leftMenu ul.submenu a:after{
	font-family: FontAwesome;
	font-style: normal;
	font-weight: normal;
	line-height: 1;
	-webkit-font-smoothing: antialiased;
	-moz-osx-font-smoothing: grayscale;
	content: "\f101";
	position:absolute;
	right:15px;
	top:7px;
	color:#098ceb;
	}
ul.leftMenu ul.submenu a:hover, ul.leftMenu ul.submenu a.active{
	color:#FFF;
	}
ul.leftMenu li a.open-submenu {
	background:
	url(../images/plus-icon.png) 94% center no-repeat,
	url(../images/dark-bg.png) top left repeat;
	}
ul.leftMenu li a.hide-submenu {
	background:
	url(../images/minus-icon.png) 94% center no-repeat,
	url(../images/dark-bg.png) top left repeat;
	}
.sidebx {
