.listing2_k {
	background:rgba(255,255,255,1.00);
	padding:10px;
	border:solid 1px rgba(207,207,207,0.74);
	border-radius:3px;
	margin-bottom:5px;
	transition:background 1s;
	position:relative;
	
}
.listing2_k:hover {
	background: rgba(241,250,228,1.00);
}
.listing2_k .btn  {
	border-radius:0;
}
.listing2_k .btn i {
	padding-right:0;
}
.listing2_k img{
	border:solid 4px #FFFFFF;
	box-shadow:0 0 2px 0 rgba(90,90,90,0.52);
	}
.listing2Icon_k{ width:65px; height:65px; float:left; margin-right:20px;}
.listing2Icon_k img {
	width:65px;
	height:65px;
	}
.listing2content_k{
	float:left;
	width:87%;
	}
.listing2_k .row .col-md-5:nth-child(2), .listing2 .row .col-md-6:nth-child(2) {
	border-right: solid 1px #c4e5fc;
	padding-left:0;
	}
.listing2_k p {line-height:22px; margin-bottom:0;}
.listing2_k strong {font-weight:600;}


