#petnavthumb_k {
	border-radius:50%;
	width:22px;
	height:22px;
	position:relative;
	/*margin:11px auto 0 auto;*/
    transition: border 0.3s ease-out, color 0.3s ease 0.5s;
	
	}
#petnavthumb_k img{
	width:20px;
	height:20px;
	border-radius:50%;
	border: 2px solid #0073b6;
    transition: border 0.3s ease-out, color 0.3s ease 0.5s;
	box-shadow:0 0 50px 0 #F8181C inset;
	}
#petnavthumb_k:before{
	content: '';
	position: absolute;
	border-top: 6px solid transparent;
	border-bottom: 6px solid transparent;	
	border-left: 6px solid #0073b6;
	display: block;
	width: 0;
	z-index: 500;
	top: 6px;
	right: -4px;
    float:left;
	}
